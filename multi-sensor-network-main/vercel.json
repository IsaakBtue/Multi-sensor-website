{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/multi-sensor-network-main/gateway-server/$1"
    },
    {
      "src": "/",
      "dest": "/multi-sensor-network-main/gateway-server/index.html"
    }
  ]
}
